{
  "name": "strapi-plugin-oauth2",
  "description": "OAuth 2.0 plugin for Strapi",
  "version": "1.0.3",
  "keywords": [],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "publish": "yarn build && yarn verify"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "@strapi/utils": "^5.31.3",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "lodash": "^4.17.21",
    "qs": "^6.14.0",
    "react-intl": "^7.1.14",
    "validator": "^13.15.23"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.31.3",
    "@strapi/typescript-utils": "^5.31.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "prettier": "^3.7.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.31.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-plugin-oauth2",
    "displayName": "OAuth 2.0",
    "description": "OAuth 2.0 plugin for Strapi"
  },
  "license": "MIT",
  "author": {
    "name": "Sorasak Siangsanan",
    "email": "siangsanan.sorasak@gmail.com",
    "url": "https://sorasak.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edwardmelody/strapi-plugin-oauth2.git"
  },
  "bugs": {
    "url": "https://github.com/edwardmelody/strapi-plugin-oauth2/issues"
  },
  "homepage": "https://github.com/edwardmelody/strapi-plugin-oauth2#readme"
}
