OAUTH_JWT_SIGN_KEY=xxxxx             # ถ้า HS256 ให้เป็น 32+ byte secret; ถ้า RS256 ให้เป็น PEM private key
OAUTH_JWT_ALG=HS256                  # หรือ RS256
OAUTH_ACCESS_TOKEN_TTL=3600
OAUTH_AUD=your-api
OAUTH_AUTH_CODE_TTL_SECONDS=300      # ระยะเวลาหมดอายุของ authorization code (วินาที)
OAUTH_LOGIN_URL=http://localhost:3000/oauth2/login          # URL สำหรับหน้า login
