{
  "kind": "collectionType",
  "collectionName": "oauth-clients",
  "info": {
    "singularName": "oauth-client",
    "pluralName": "oauth-clients",
    "displayName": "OAuth Client"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "clientId": {
      "type": "string",
      "unique": true,
      "configurable": false
    },
    "clientSecret": {
      "type": "string",
      "configurable": false
    },
    "clientSecretMasked": {
      "type": "string",
      "configurable": false
    },
    "clientSecretHash": {
      "type": "string",
      "private": true,
      "configurable": false
    },
    "jwtAlg": {
      "type": "enumeration",
      "required": true,
      "enum": ["HS256", "RS256", "ES256"],
      "configurable": false
    },
    "publicKey": {
      "type": "text",
      "configurable": false
    },
    "privateKey": {
      "type": "text",
      "configurable": false
    },
    "privateKeyMasked": {
      "type": "text",
      "configurable": false
    },
    "name": {
      "type": "string",
      "required": true,
      "configurable": false
    },
    "scopes": {
      "type": "component",
      "component": "oauth-client.scope",
      "repeatable": true,
      "configurable": false
    },
    "redirectUris": {
      "type": "component",
      "component": "oauth-client.redirect-uri",
      "repeatable": true,
      "configurable": false
    },
    "active": {
      "type": "boolean",
      "default": true,
      "configurable": false
    },
    "meta": {
      "type": "json",
      "configurable": false
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "configurable": false
    },
    "clientType": {
      "type": "enumeration",
      "required": true,
      "enum": ["CONFIDENTIAL", "PUBLIC"],
      "configurable": false
    },
    "createdType": {
      "type": "enumeration",
      "required": true,
      "enum": ["USER", "BACK_OFFICE"],
      "configurable": false
    }
  }
}
