{
  "kind": "collectionType",
  "collectionName": "oauth-users",
  "info": {
    "singularName": "oauth-user",
    "pluralName": "oauth-users",
    "displayName": "OAuth User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "userDocumentId": {
      "type": "string",
      "required": true,
      "configurable": false
    },
    "clientId": {
      "type": "string",
      "required": true,
      "configurable": false
    },
    "scopes": {
      "type": "component",
      "component": "oauth-user.scope",
      "repeatable": true,
      "configurable": false
    },
    "apiTokenId": {
      "type": "integer",
      "min": 1,
      "required": true,
      "configurable": false
    },
    "apiTokenAccessKey": {
      "type": "text",
      "required": true,
      "private": true,
      "searchable": false,
      "configurable": false
    },
    "client": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::strapi-plugin-oauth2.oauth-client",
      "configurable": false
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "configurable": false
    }
  }
}
