{
  "kind": "collectionType",
  "collectionName": "oauth-authorization-codes",
  "info": {
    "singularName": "oauth-authorization-code",
    "pluralName": "oauth-authorization-codes",
    "displayName": "OAuth Authorization Code"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "codeHash": {
      "type": "string",
      "private": true,
      "configurable": false
    },
    "client": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::strapi-plugin-oauth2.oauth-client",
      "configurable": false
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "configurable": false
    },
    "scopes": {
      "type": "component",
      "component": "oauth-authorization-code.scope",
      "repeatable": true,
      "configurable": false
    },
    "redirectUri": {
      "type": "string",
      "required": true,
      "configurable": false
    },
    "codeChallenge": {
      "type": "string",
      "configurable": false
    },
    "codeChallengeMethod": {
      "type": "string",
      "configurable": false
    },
    "expiresAt": {
      "type": "datetime",
      "required": true,
      "configurable": false
    },
    "usedAt": {
      "type": "datetime",
      "configurable": false
    },
    "meta": {
      "type": "json",
      "configurable": false
    }
  }
}
